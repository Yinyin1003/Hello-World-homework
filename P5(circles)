function setup() {
  createCanvas(600, 500); 
  noLoop();  
}

function draw() {
  background(0);  

  // 画 30 个随机位置的圆
  for (var i = 0; i < 20; i++) {
    let x = random(width);  
    let y = random(height);  
    let radius = 30;  
    fill(random(255),random(255),random(255));  
    noStroke();  
    circle(x, y, 20);  
  }
}
